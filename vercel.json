{
    "builds": [
        {
            "src": "pay2live",
            "use": "@vercel/python"
        },
        {
            "src": "pay2live/static/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "pay2live"
        }
    ]
}